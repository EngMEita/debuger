{
  "name": "meita/debuger",
  "description": "Laravel package that hides technical error details from end users and emails full reports.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mohamed A. Eita",
      "email": "maa1987@hotmail.com"
    }
  ],
  "keywords": [
    "laravel",
    "exception",
    "error",
    "handler",
    "email"
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
    "illuminate/mail": "^8.0|^9.0|^10.0|^11.0",
    "illuminate/contracts": "^8.0|^9.0|^10.0|^11.0",
    "illuminate/foundation": "^8.0|^9.0|^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Meita\\Debuger\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Meita\\Debuger\\DebugerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
